{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "aflnet-live555",
      "type": "cppdbg",
      "request": "launch",
      "args": [
        "-d",
        "-i",
        "$AFLNET/tutorials/live555/in-rtsp",
        "-o",
        "out-live555",
        "-N",
        "tcp://127.0.0.1/8554",
        "-x",
        "$AFLNET/tutorials/live555/rtsp.dict",
        "-P",
        "RTSP",
        "-D",
        "10000",
        "-q",
        "4",
        "-s",
        "4",
        "-E",
        "-K",
        "-R",
        "-r",
        "404",
        "./testOnDemandRTSPServer",
        "8554"
      ],
      "environment": [
        {
          "name": "AFLNET",
          "value": "/home/ly/Projects/SMGfuzz"
        },
        {
          "name": "WORKDIR",
          "value": "/home/ly/Projects/afl_net_run"
        },
        {
          "name": "PATH",
          "value": "/home/ly/Projects/SMGfuzz/:$PATH"
        },
        {
          "name": "AFL_PATH",
          "value": "/home/ly/Projects/SMGfuzz/"
        }
      ],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "/home/ly/Projects/afl_net_run/live555/testProgs",
      "program": "/home/ly/Projects/SMGfuzz/afl-fuzz",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "aflnet-ftp",
      "type": "cppdbg",
      "request": "launch",
      "args": [
        "-d",
        "-i",
        "$AFLNET/tutorials/lightftp/in-ftp",
        "-o",
        "out-lightftp",
        "-m",
        "none",
        "-N",
        "tcp://127.0.0.1/2200",
        "-x",
        "$AFLNET/tutorials/lightftp/ftp.dict",
        "-P",
        "RTSP",
        "-D",
        "10000",
        "-q",
        "3",
        "-s",
        "3",
        "-E",
        "-R",
        "-c",
        "./ftpclean.sh",
        "./fftp",
        "fftp.conf",
        "2200"
      ],
      "environment": [
        {
          "name": "AFLNET",
          "value": "/home/ly/Projects/SMGfuzz"
        },
        {
          "name": "WORKDIR",
          "value": "/home/ly/Projects/afl_net_run"
        },
        {
          "name": "PATH",
          "value": "/home/ly/Projects/SMGfuzz/:/usr/bin/:$PATH"
        },
        {
          "name": "AFL_PATH",
          "value": "/home/ly/Projects/SMGfuzz/"
        }
      ],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "/home/ly/Projects/afl_net_run/LightFTP/Source/Release",
      "program": "/home/ly/Projects/SMGfuzz/afl-fuzz",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
